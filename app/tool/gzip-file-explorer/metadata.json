        "stateKey": "inputFileId"
      }
    ]
  },
  "outputConfig": {
    "transferableContent": [
      {
        "dataType": "reference",
        "stateKey": "decompressedFileId"
      }
    ]
  },
  "includeInSitemap": true,
  "status": "stable"
}