{
  "title": "Gzip File Explorer",
  "directive": "gzip-file-explorer",
  "description": "Allows users to explore the contents of gzip compressed files without needing any server-side assistance.",
  "inputConfig": {
    "acceptsMimeTypes": ["application/gzip", "application/x-gzip"],
    "stateFiles": [{ "stateKey": "selectedGzipFileId" }]
  },
  "outputConfig": {
    "transferableContent": [
      {
        "dataType": "reference",
        "stateKey": "decompressedFileId"
      }
    ]
  },
  "includeInSitemap": true,
  "status": "stable"
}