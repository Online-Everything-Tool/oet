{
  "title": "Json Validate Format",
  "directive": "json-validate-format",
  "description": "Easily validate and format JSON data.",
  "urlStateParams": [
    {
      "paramName": "json",
      "type": "string",
      "defaultValue": ""
    },
    {
      "paramName": "indent",
      "type": "number",
      "defaultValue": 2
    }
  ],
  "inputConfig": {
    "acceptsMimeTypes": ["application/json"],
    "stateFiles": "none"
  },
  "outputConfig": {
    "transferableContent": [
      {
        "dataType": "inline",
        "stateKey": "outputValue",
        "mimeType": "application/json"
      }
    ]
  },
  "includeInSitemap": true,
  "status": "stable"
}
